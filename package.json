{
  "name": "rideplanner",
  "version": "0.0.1",
  "description": "Starter for express 4 w/ postgres and passportJS for auth",
  "main": "server.js",
  "scripts": {
    "clean": "rimraf dist",
    "start": "node server.js",
    "watch": "webpack --watch && npm run watch:css",
    "dev": "npm run build && nodemon server.js",
    "eslint:server": "eslint models routes server.js",
    "build": "npm run clean && webpack && npm run eslint:server && npm run build:css",
    "build:css": "node-sass --include-path scss front-end/src/scss/app.scss dist/css/app.css",
    "watch:css": "nodemon -e scss -x \"npm run build:css\"",
    "test": "mocha --compilers js:babel/register --recursive",
    "test:watch": "npm test -- --watch",
    "db:migrate": "sequelize db:migrate",
    "postinstall": "npm run build"
  },
  "dependencies": {
    "axios": "^0.17.1",
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.2.1",
    "babel-loader": "^7.1.2",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-imports": "^1.4.1",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "babel-runtime": "^6.26.0",
    "bcrypt": "^1.0.3",
    "body-parser": "^1.18.2",
    "config": "^1.29.0",
    "connect-flash": "^0.1.1",
    "debug": "^3.1.0",
    "del": "^3.0.0",
    "eslint": "^4.15.0",
    "eslint-loader": "^1.9.0",
    "eslint-plugin-react": "^7.5.1",
    "express": "~4.16.x",
    "express-session": "^1.15.6",
    "history": "^4.7.2",
    "jade": "^1.11.0",
    "morgan": "^1.9.0",
    "mustbe": "^0.4.0",
    "node-config": "0.0.2",
    "node-sass": "^4.7.2",
    "passport": "^0.4.0",
    "passport-local": "^1.0.0",
    "pg": "^7.4.1",
    "pg-hstore": "^2.3.2",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-loader": "^2.4.2",
    "react-redux": "^5.0.6",
    "react-router": "^4.2.0",
    "redux": "^3.7.2",
    "redux-thunk": "^2.2.0",
    "rimraf": "^2.6.2",
    "sequelize": "^3.17.3",
    "sequelize-cli": "^2.3.1",
    "webpack": "^3.10.0",
    "webpack-dev-middleware": "^2.0.4",
    "webpack-hot-middleware": "^2.21.0"
  },
  "engines": {
    "node": "9.x"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/krousseau/rideplanner.git"
  },
  "keywords": ["node", "heroku", "express", "postgres", "mustbe", "sequelize", "passportJs", "react"],
  "license": "MIT",
  "devDependencies": {}
}
